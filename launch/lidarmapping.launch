<launch>

  <!-- start the aerial lidar mapping node -->
 <node pkg="aerial_lidar_mapping" type="aerial_lidar_mapping_node" name="AerialLidarMap" output="screen">
</node>

  <!-- Start the set gps reference node -->
  <node pkg="geodetic_utils" type="set_gps_reference_node" name="set_gps_reference" output="screen">
    <remap from="/gps" to="/dji_sdk/gps_position" />
  </node>

  <!-- Start the gps to pose conversion node -->
 <node pkg="geodetic_utils" type="gps_to_pose_conversion_node" name="gps_to_pose_conversion" output="screen">
   <remap from="/gps" to="/dji_sdk/gps_position" />
   <remap from="/imu" to="/dji/imu_corrected" />
   <param name="publish_pose" value="true" />
   <param name="tf_child_frame_id" value="body_frame" />
</node>

</launch>
